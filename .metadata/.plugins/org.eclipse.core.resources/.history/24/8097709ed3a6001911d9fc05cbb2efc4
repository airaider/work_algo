import java.util.Scanner;

public class Solution {

	private static Scanner scan;

	public static void main(String[] args) {
		scan = new Scanner(System.in);  
		//message = scan.nextLine();   
		//kilometer = scan.nextInt();   
		int T;
		T = scan.nextInt();
		int m;
		
		
		for(int i=0;i<T;i++) {
			int N;
			N = scan.nextInt();
			int[] arr = new int[N];
			for(int j=0;j<N;j++) {
				arr[j] = scan.nextInt();
				
			}
			m=check(arr,N,i+1);
			System.out.println("#"+(i+1)+" " +m);
		}
		
		
		
	}

	private static int check(int[] arr, int n, int t) {
		// TODO Auto-generated method stub
		int count=0;
		int max=0;
        int temp;
		for(int i=0;i<n-1;i++) {
			count=1;
            temp=arr[i];
			for(int j=i+1;j<n;j++) {
				if(arr[j]>=arr[i]) {
					count+=1;
                    temp=arr[j];
				}
				if (count>max){
					max=count;
				}
			}
		}
		
		return max;
		
	}

	

}
